{
  "name": "@munchi_oy/core",
  "version": "1.2.6",
  "description": "Munchi Core SDK - Core utilities and types",
  "license": "UNLICENSED",
  "publishConfig": {
    "access": "restricted"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "react-native": "index.ts",
  "files": [
    "dist",
    "src",
    "index.ts",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "react-native": "./index.ts"
    }
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "version:sync": "node scripts/sync-version.mjs",
    "prebuild": "pnpm version:sync",
    "build:runtime": "tsup",
    "build:types": "tsc -p tsconfig.build.json",
    "build": "pnpm run build:runtime && pnpm run build:types",
    "yalc:publish": "pnpm build && yalc publish --push",
    "dev": "tsup --watch",
    "prepare": "npm run build",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "axios": ">=1.0.0"
  }
}

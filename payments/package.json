{
  "name": "@munchi/payments",
  "version": "1.1.7",
  "description": "Munchi Payments SDK - Payment processing utilities",
  "license": "UNLICENSED",
  "publishConfig": {
    "access": "restricted"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "react-native": "index.ts",
  "files": [
    "dist",
    "src",
    "index.ts",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "react-native": "./index.ts"
    }
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "version:sync": "node scripts/sync-version.mjs",
    "prebuild": "pnpm version:sync",
    "build": "tsup index.ts --format cjs,esm --dts --clean",
    "yalc:publish": "pnpm build && yalc publish --push",
    "dev": "tsup index.ts --format cjs,esm --watch --dts",
    "prepare": "npm run build",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@munchi/core": ">=1.0.0",
    "axios": ">=1.0.0"
  },
  "devDependencies": {
    "@munchi/core": "workspace:*",
    "@types/jest": "^30.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}

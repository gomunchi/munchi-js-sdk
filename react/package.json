{
  "name": "@munchi/react",
  "version": "1.1.8",
  "description": "Munchi React SDK - React Context and Hooks for Munchi SDKs",
  "license": "UNLICENSED",
  "publishConfig": {
    "access": "restricted"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "react-native": "index.ts",
  "files": [
    "dist",
    "src",
    "index.ts",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "react-native": "./index.ts"
    }
  },
  "scripts": {
    "version:sync": "node scripts/sync-version.mjs",
    "prebuild": "pnpm version:sync",
    "build:runtime": "tsup",
    "build:types": "tsc -p tsconfig.build.json",
    "build": "pnpm run build:runtime && pnpm run build:types",
    "yalc:publish": "pnpm build && yalc publish --push",
    "dev": "tsup --watch",
    "prepare": "npm run build",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {},
  "peerDependencies": {
    "@munchi/core": ">=1.0.0",
    "@munchi/payments": ">=1.0.0",
    "react": ">=18.2.0",
    "react-native": ">=0.76.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  },
  "devDependencies": {
    "@munchi/core": "workspace:*",
    "@munchi/payments": "workspace:*",
    "@types/react": "^18.3.0",
    "react": "^18.3.0",
    "react-native": "0.76.9",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
